  "templating": {
    "list": [
      {
        "name": "site_name",
        "type": "query",
        "label": "Site",
        "datasource": {
          "type": "elasticsearch",
          "uid": "elasticsearch-datasource"
        },
        "query": "{\"find\": \"terms\", \"field\": \"site.name.keyword\"}",
        "multi": true,
        "includeAll": true,
        "allValue": "*",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "refresh": 1
      },
      {
        "name": "interval",
        "type": "interval",
        "label": "Interval",
        "auto": true,
        "auto_count": 30,
        "auto_min": "1m",
        "options": [
          {
            "selected": false,
            "text": "1m",
            "value": "1m"
          },
          {
            "selected": true,
            "text": "5m",
            "value": "5m"
          },
          {
            "selected": false,
            "text": "15m",
            "value": "15m"
          },
          {
            "selected": false,
            "text": "1h",
            "value": "1h"
          }
        ],
        "current": {
          "text": "5m",
          "value": "5m"
        }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "datasource": {
          "type": "elasticsearch",
          "uid": "elasticsearch-datasource"
        },
        "enable": true,
        "name": "Deployment Events",
        "iconColor": "blue",
        "query": "tags:deployment"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "type": "stat",
      "title": "Success Rate (Last Hour)",
      "targets": [
        {
          "datasource": {
            "type": "elasticsearch",
            "uid": "elasticsearch-datasource"
          },
          "query": "site.name.keyword:$site_name",
          "metrics": [
            {
              "id": "1",
              "type": "avg",
              "field": "status.success"
            }
          ],
          "timeField": "@timestamp",
          "bucketAggs": [
            {
              "id": "2",
              "type": "date_histogram",
              "field": "@timestamp",
              "settings": {
                "interval": "auto"
              }
            }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 0.95,
                "color": "yellow"
              },
              {
                "value": 0.99,
                "color": "green"
              }
            ]
          },
          "decimals": 2
        }
      },
      "options": {
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "mean"
          ]
        }
      }
